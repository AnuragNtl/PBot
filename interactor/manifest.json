{

  "manifest_version": 2,
  "name": "Bot",
  "version": "1.0",

  "description": "PBot",

"applications": {
    "gecko": {
      "id": "pbot@example.com",
      "strict_min_version": "50.0"
    }
  },
  "background": {
    "scripts": ["bot.js"]
  },
  "content_scripts":[
  {"matches":["*://*/*"],"js":["content.js"]}
  ]
  ,
  "permissions": ["nativeMessaging"]

}
